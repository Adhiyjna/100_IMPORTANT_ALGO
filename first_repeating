#include<bits/stdc++.h>
using namespace std;
int main()
{int dup[100];
    int n,i,j;
    cout<<"enter the no of elements";
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
   int k=0;
   for(i=0;i<n;i++)
   {
       for(j=i+1;j<n;j++)
       {
           if(a[i]==a[j])
           {
               dup[k++]=a[i];
           }
       }
   }
   
   for(i=0;i<k;i++)
   {
       cout<<dup[0];
       break;
   }
   
 
   
}
