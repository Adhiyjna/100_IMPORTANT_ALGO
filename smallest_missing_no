
#include <iostream>

using namespace std;

int main()
{  int n,i;
    cout<<"enter the no of elements";
    cin>>n;
    int a[n];
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    for(i=0;i<n;i++)
    {
        if(a[i+1]-a[i]>1)
        {
            cout<<a[i]+1;
            break;
        }
    }
    
}
