#include<bits/stdc++.h>
using namespace std;

int main()
{
    int i,n;
    
    cout<<"enter the no of elements";
    cin>>n;
    int a[n];
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    int freq[100]={0};
    
    for(i=0;i<n;i++)
    {
        freq[a[i]]++;
    }
    
    for(i=0;i<100;i++)
    {
        if(freq[i])
        {
            cout<<i<<freq[i]<<endl;
        }
    }
    
   
    
    
}
