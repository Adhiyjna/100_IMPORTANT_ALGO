#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,n,a[10];
    int sum=0;
    cout<<"enter the number of elements";
    cin>>n;
    
   for( i=0;i<n;i++)
    {
       cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
        sum+=a[i];
    }
   
    int total=0;
    for(i=0;i<n;i++)
    {
    total=(n+1)*(n+2)/2;
    }
    
    int c=total-sum;
    
    cout<<c;
    
   
}
