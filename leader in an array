#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,j,i;
    cout<<"enter the no of elements";
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]<=a[j])
            {
                break;
            }
        }
        if(j==n)
        {
            cout<<a[i];
        }
    }
    
    
}
