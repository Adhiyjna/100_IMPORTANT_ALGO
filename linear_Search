#include <iostream>

using namespace std;

int main()
{
    int n,i,a[100];
    int x;
    cout<<"enter the no of elements";
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        cin>>a[i];        
    }
    cout<<"element to be found";
    cin>>x;
    
    for(i=0;i<n;i++)
    {
        if(a[i]==x)
        {
            cout<<"element founded";
        }
       else
       {
           cout<<"not founded";
           break;
       }
    }
    
   /* for(int i=0;i<n;i++)
    {
        cout<<a[i]<<endl;
    }*/
}
