#include <iostream>

using namespace std;

int main()
{ 
    //finding fixed point in an array
    int n,i;
    cout<<"enter the no of elements";
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int x;
    for(i=0;i<n;i++)
    {
        if(a[i]==x)
        {
            cout<<a[i];
        }
    }
    
    
   
    
}
